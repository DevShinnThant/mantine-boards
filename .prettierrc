{
  "arrowParens": "always",
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "importOrder": [
    "^react$",
    "^vite$",
    "^react-router-dom$",
    "^@mantine/(.*)$",
    "mantine-datatable",
    "<THIRD_PARTY_MODULES>",
    "^~/hooks/(.*)$",
    "^~/utils/(.*)$",
    "^~/store(/*.*)$",
    "^~/client-store(/*.*)$",
    "^~/assets/(.*)$",
    "^~/types/(.*)$",
    "^~/components/(.*)$",
    "^~/layouts/(.*)$",
    "^~/styles/(.*)$",
    "^~/dashboards/(.*)$",
    "^~/routes/(.*)$",
    "^[./]",
    "^[../]"
  ],
  "importOrderSeparation": false,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
